<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/16"
      fx:controller="geardesigner.AnyCircleController">
    <children>
        <HBox alignment="CENTER_LEFT" spacing="10.0">
            <children>
                <Label alignment="TOP_CENTER" text="任一圆直径(mm)"/>
                <TextField fx:id="tfDoubleAnyCircle" prefWidth="120.0"/>
                <Button fx:id="btCalAnyCircle" text="计算"/>
            </children>
        </HBox>
        <AnchorPane fx:id="APaneAnyCircle" layoutX="10.0" layoutY="10.0"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
