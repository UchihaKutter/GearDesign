<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<fx:root stylesheets="@ParamTable.css" type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <VBox fx:id="vBox" alignment="TOP_CENTER" StackPane.alignment="CENTER">
        <children>
            <Text fx:id="tTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
            <HBox alignment="CENTER_LEFT">
                <children>
                    <Label fx:id="lCol0" text="Column0" />
                    <Label fx:id="lCol1" layoutX="10.0" layoutY="10.0" text="Column1" />
                    <Label fx:id="lCol2" layoutX="10.0" layoutY="10.0" text="Column2" />
                </children>
            </HBox>
        </children>
    </VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
